import java.util.Stack;

public class MatchBrackets {

	public String match(String input){
		String op = "NO";
		Stack<Character> stk = new Stack<Character>();
		char temp;
		//stk.push(input.charAt(0));
		for (int i=0; i<input.length(); i++){
			temp = input.charAt(i);
			if(i==0)
				stk.push(temp);
			else{
				if(stk.peek().equals(temp)){
					stk.pop();
				}
				else
					stk.push(temp);
			}
		}
		if(stk.isEmpty())
			op = "YES";
		
		return op;
	}
	
}
