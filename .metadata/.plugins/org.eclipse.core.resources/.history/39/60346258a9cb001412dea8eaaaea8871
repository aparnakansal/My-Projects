package crud.servlets;

import java.util.HashMap;
import java.util.Map;

public class Table {
	
	private Map<String, HomePage> user = new HashMap<String, HomePage>();

	public HomePage getUser(String id) {
		return this.user.get(id);	
	}

	public void setUser(String id, HomePage details) {
		this.user.put(id, details);	
	}
}
