<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="greeter" class="com.helloworld.javabeans.GreeterClass">
		<property name="names" value="Alice"> </property>
	</bean>
	
	<bean id="greeter2" class="com.helloworld.javabeans.GreeterClass">
		<property name="names">
			<list value-type="java.lang.String">
				<value>Alice</value>
				<value>Bob</value>
				<value>Carl</value>
			</list>
		</property>
	</bean>

</beans>